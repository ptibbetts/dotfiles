{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "comma-dangle": 0,
    "import": 0,
    "max-len": 0,
    "no-console": 1,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "plugins": [
    "html",
    "prettier"
  ]
}